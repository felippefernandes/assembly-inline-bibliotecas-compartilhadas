#include <stdio.h>

int main() {

	int op;

	printf("\n\n\n\t\tTRABALHO 2 - PLM\n\n");
	printf("\nEscolha a funcao desejada: \n");
	printf("1 - SOMA\n");
	printf("2 - SUBTRACAO\n");
	printf("3 - MULTIPLICACAO\n");
	printf("4 - DIVISAO\n");
	printf("5 - RESTO DA DIVIS√ÉO\n");
	printf("6 - FUNCAO AND\n");
	printf("7 - FUNCAO OR\n");
	printf("8 - FUNCAO XOR\n");
	printf("9 - FUNCAO NOT\n");

	scanf("%d", &op);



	switch (op) {

		case 1:
			return soma(5,3);
			break;

		case 2:
			return subtracao(5,6);
			break;

		case 3:
			return multiplicacao(8,5);
			break;

		case 4:
			return divisao(13,3);
			break;

		case 5:
			return resto(14,3);
			break;
		
		case 6:
			return e_logico(1,1);
			break;

		case 7:
			return ou_logico(1,1);
			break;

		case 8:
			return ou_exclusivo(1,1);
			break;

		case 9:
			return nao_logico(1);
			break;

	
		default:
			printf("\nEncerrando...\n\n\n");

	}




	

}
